	<subDataset name="Factura_Detalle" uuid="435cc272-bb5c-4059-bb28-dfd9908697fd">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="DataAdapter.xml"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="ID_FACTURA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
	fd.cantidad "Cantidad", p.producto "Producto", concat('$', format(fd.precio_venta,0)) "Precio Unitario", concat('$', format((fd.precio_venta * fd.cantidad),0)) "Precio Total"
FROM
	tcr.factura_detalle fd
	INNER JOIN tcr.producto p
	ON fd.id_producto = p.id_producto
WHERE
	fd.id_factura = $P{ID_FACTURA}
UNION
SELECT
	(dd.cantidad * -1), p.producto, '-', concat('-$', format((dd.precio_venta * dd.cantidad),0))
FROM
	tcr.devolucion_detalle dd
	INNER JOIN tcr.producto p
	ON dd.id_producto = p.id_producto
WHERE
	dd.id_factura = $P{ID_FACTURA}
ORDER BY producto, cantidad DESC;]]>
		</queryString>
		<field name="Cantidad" class="java.lang.Integer">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="Producto" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="Precio Unitario" class="java.lang.String"/>
		<field name="Precio Total" class="java.lang.String"/>
	</subDataset>
	<parameter name="NUMERO_FACTURA_VENTA" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	concat('$', format(fc.valor_neto - IFNULL(dc.valor_neto, 0), 1)) "VALOR NETO", concat('$', format(fc.valor_iva - IFNULL(dc.valor_iva, 0), 1)) "IVA", concat('$', format(fc.valor_total - IFNULL(dc.valor_total, 0), 1)) "TOTAL_FACTURA"
FROM
	tcr.factura_cabecera fc
	LEFT JOIN tcr.devolucion_cabecera dc
	ON fc.id_factura = dc.id_factura
WHERE
	fc.id_factura = $P{NUMERO_FACTURA_VENTA};]]>
	</queryString>
	<field name="VALOR NETO" class="java.lang.String"/>
	<field name="IVA" class="java.lang.String"/>
	<field name="TOTAL_FACTURA" class="java.lang.String"/>
